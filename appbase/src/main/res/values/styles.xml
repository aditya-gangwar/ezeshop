    <style name="MyTheme" parent="MyTheme_v16"/>

    <style name="MyTheme_v16" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->

        <item name="colorPrimary">@color/primary</item>
        <item name="colorPrimaryDark">@color/primary_dark</item>
        <item name="colorAccent">@color/accent</item>

        <item name="android:windowBackground">@color/white</item>

        <item name="android:textColorPrimary">@color/primary_text</item>
        <item name="android:textColorSecondary">@color/secondary_text</item>
        <item name="android:textColorLink">@color/link_blue</item>
        <item name="android:textColorHint">@color/hint</item>

        <item name="drawerArrowStyle">@style/DrawerArrowStyle</item>
        <item name="toolbarNavigationButtonStyle">@style/MyNavButtonStyle</item>

        <!-- The style for the widgets on the ActionBar. -->
        <!--item name="actionBarWidgetTheme">@style/WidgetStyle</item-->

    </style>

    <style name="MyTextInputLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="errorEnabled">true</item>
    </style>

    <style name="MySpinner" parent="Widget.AppCompat.Spinner.Underlined">
